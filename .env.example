# Основные настройки приложения
NODE_ENV=development
PORT=3000
LOG_LEVEL=info
APP_ENCRYPTION_KEY=your-encryption-key-here

# Настройки базы данных PostgreSQL
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=nexus
DATABASE_PASSWORD=your-database-password
DATABASE_NAME=nexus

# Настройки Telegram Bot
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_ADMIN_IDS=123456789,987654321

# Настройки OpenAI API
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o

# Настройки LLM системы
LLM_ACTIVE_PROVIDER=llama
LLM_FALLBACK_ENABLED=true
LLM_CACHE_ENABLED=true

# Настройки Llama провайдера
LLM_LLAMA_ENABLED=true
LLM_LLAMA_ENDPOINT=http://localhost:8080
LLM_LLAMA_API_KEY=your-llama-api-key
LLM_LLAMA_MODEL=llama-4-70b

# Настройки OpenAI провайдера
LLM_OPENAI_ENABLED=true

# Административные настройки
ADMIN_API_KEY=your-admin-api-key

# CORS настройки
CORS_ORIGIN=*
