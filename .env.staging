# Основные настройки приложения - STAGING
NODE_ENV=staging
PORT=3000
LOG_LEVEL=debug
APP_ENCRYPTION_KEY=YOUR_STAGING_ENCRYPTION_KEY

# Настройки базы данных PostgreSQL - STAGING
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=nexus_staging
DB_PASSWORD=YOUR_STAGING_DB_PASSWORD
DB_DATABASE=nexus_staging
DB_SYNCHRONIZE=false

# Настройки Telegram Bot - STAGING
TELEGRAM_BOT_TOKEN=YOUR_STAGING_TELEGRAM_BOT_TOKEN
TELEGRAM_ADMIN_IDS=YOUR_ADMIN_USER_IDS

# Настройки OpenAI API - STAGING
OPENAI_API_KEY=YOUR_STAGING_OPENAI_API_KEY
OPENAI_MODEL=gpt-4o-mini

# Настройки LLM системы - STAGING
LLM_ACTIVE_PROVIDER=llama
LLM_FALLBACK_ENABLED=true
LLM_CACHE_ENABLED=true

# Настройки Llama/Ollama провайдера - STAGING
LLM_LLAMA_ENABLED=true
OLLAMA_BASE_URL=http://localhost:11434
LLM_LLAMA_API_KEY=
LLM_LLAMA_MODEL=llama3.2:3b

# Настройки OpenAI провайдера - STAGING
LLM_OPENAI_ENABLED=true

# Административные настройки - STAGING
ADMIN_API_KEY=YOUR_STAGING_ADMIN_API_KEY

# CORS настройки - STAGING
CORS_ORIGIN=https://staging.yourdomain.com
